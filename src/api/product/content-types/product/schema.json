{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 51,
      "minLength": 3
    },
    "Slug": {
      "type": "uid",
      "required": true,
      "maxLength": 130,
      "minLength": 3
    },
    "Description": {
      "type": "text"
    },
    "Image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "T-shirts",
        "Sticker",
        "mug"
      ],
      "default": "T-shirts",
      "required": true
    },
    "Size": {
      "type": "string",
      "maxLength": 71
    },
    "Colour": {
      "type": "enumeration",
      "enum": [
        "Red ",
        "Green ",
        "Blue",
        "white",
        "Black"
      ]
    },
    "Prize": {
      "type": "integer"
    },
    "AvailableQty": {
      "type": "integer",
      "required": true
    }
  }
}
